# Copy to secrets.env and fill in values. Never commit secrets.env.

# Telegram Bot (for daily forecast notifications)
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_CHAT_ID=your_chat_id

# Wansoft POS System (required for transfer/sales/payments ETL)
WS_USER=your_email
WS_PASS=your_password
WS_BASE=https://www.wansoft.net/Wansoft.Web

# Google Drive (for Zapier upload)
GOOGLE_DRIVE_FOLDER_ID=your_folder_id
